    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "sinon": "^2.4.1",
